{
  "name": "mantlejs",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run test:coverage && gulp",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testMatch": [
      "**/*.spec.ts"
    ],
    "coverageDirectory": ".coverage"
  },
  "author": "Mihail Cristian Dumitru",
  "license": "MIT",
  "dependencies": {
    "lodash.get": "^4.4.2",
    "lodash.isequal": "^4.5.0",
    "lodash.set": "^4.3.2"
  },
  "devDependencies": {
    "@types/node": "^10.5.4",
    "@types/jest": "^23.3.1",
    "@types/lodash.get": "^4.4.4",
    "@types/lodash.set": "^4.3.4",
    "@types/lodash.isequal": "^4.5.3",
    "typescript": "^2.9.2",
    "gulp": "^4.0.0",
    "del": "^3.0.0",
    "jest": "^23.4.2",
    "ts-jest": "^23.0.1"
  }
}
